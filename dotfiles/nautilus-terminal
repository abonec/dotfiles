[general]
#Высота терминала (кол-во строк)
def_term_height=4
#Отображение терминала в новых окнах по-умолчанию (1: включено, 0: выключено)
def_visible=0
#Позиция терминала (1: вверху окна, 0: внизу окна)
term_on_top=1
[terminal]
#Используемая оболочка (по-умолчанию используется стандартная для пользователя оболочка, если Вы не понимаете о чем речь - не надо ничего менять)
shell=/bin/bash
