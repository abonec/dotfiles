alias cd_customers='cd ~/work/customers_zone'
alias cd_core='cd ~/work/crowdsystems'
alias cd_moderators='cd ~/work/moderation'
alias server_core='CORE= core && rails s'
alias server_customers='CUSTOMERS= customers && rails s -p3001'
alias server_moderation='MODERATION= moderators && rails s -p3002'
alias console_customer='CUSTOMERS= customers && rails c'
alias console_core='CORE= core && rails c'
alias console_moderation='MODERATION= moderators && rails c'
alias deploy_dev='git push -f dokku-dev HEAD:master'
